        var dayNight = moment().hour() < 6 ? "night" : "day";

        googletag.cmd.push(function () {
            

            googletag.defineSlot('/21703950087/1x1_cijeli_index', [1, 1], 'div-gpt-ad-1535635144958-0').addService(googletag.pubads());
            googletag.defineSlot('/21703950087/1x1_test', [1, 1], 'div-gpt-ad-1542708593084-0').addService(googletag.pubads());

            googletag.pubads().enableSingleRequest();
            googletag.pubads().collapseEmptyDivs();
            googletag.pubads().setTargeting('day/night', dayNight);
            googletag.pubads().setTargeting('referer', '');

            googletag.enableServices();
        });

});
pbjs.addAdUnits(dfpPrebidUnits);
pbjs.requestBids({
bidsBackHandler: dfpPrebidSendAdServerRequest
});
});
setTimeout(function() {
dfpPrebidSendAdServerRequest();
}, dfpPrebidTimeout);
}
}
//setup and initialize dfp adunits
googletag.cmd.push(function() {
// asinkrono renderiranje
if (dfpAsync === true) {
googletag.pubads().enableAsyncRendering();
}
// dohvati sve u jednom requestu
if (dfpSingleRequest === true) {
googletag.pubads().enableSingleRequest();
}
// smanji prazne div elemente
if (dfpCollapseEmpty === true) {
googletag.pubads().collapseEmptyDivs();
}
// isključi inicijalno učitavanje
if (dfpDisableInitialLoad === true) {
googletag.pubads().disableInitialLoad();
}
// osvježi sve jedinice koje nisu lazy loadane i nisu u prebidu
if ((dfpDisableInitialLoad === true) && (dfpRefreshUnits.length > 0)) {
googletag.pubads().refresh(dfpRefreshUnits);
}
// izvršava se nakon što je pojedina oglasna jedinica renderirana, idealno mjesto za dodavanje klasa
googletag.pubads().addEventListener('slotRenderEnded', function(event) {
if (!event.isEmpty) {
// provjeri koja se točno jedinica renderirala
var slotId = event.slot.getSlotElementId();
var isLoaded = !event.isEmpty;
dfpUnits[dfpCurrentPageType][slotId]['loaded'] = isLoaded;
// dohvati podatke o renderiranoj oglasnoj jedinici
var unitData = dfpUnits[dfpCurrentPageType][slotId];
// stavi dodatne klase na kontejnere oglasnih jedinica gdje je potrebno
if (unitData['sizeType'] === 'aside') {
if (dfpWindowWidth > dfpMobileMaxWidth) {
var elementExtraClasses = ' dfp-box';
// stari nazivi klasa, trebaju se zamijeniti univerzalnima
if ((dfpName === 'jl') || (dfpName === 'sn')) {
elementExtraClasses = ' jl-banner-box';
}
var bannerElementWrapper = document.getElementById(unitData['container']);
if (bannerElementWrapper !== null) {
bannerElementWrapper.className += elementExtraClasses;
}
}
} else if (unitData['sizeType'] === 'pagebreak') {
var elementExtraClasses = ' dfp-space';
// stari nazivi klasa, trebaju se zamijeniti univerzalnima
if ((dfpName === 'jl') || (dfpName === 'sn')) {
elementExtraClasses = ' jl-banner-space';
}
var bannerElementWrapper = document.getElementById(unitData['container']);
if (bannerElementWrapper !== null) {
bannerElementWrapper.className += elementExtraClasses;
}
// fix za in article jedinice na sportskim novostima
if ((dfpName === 'sn') && (unitData['unit'] !== 'ia_1') && (unitData['unit'] !== 'ia_2') && (unitData['unit'] !== 'ia_3')) {
var parentObj;
if (checkIfNodeInPage(bannerElementWrapper.parentNode)) {
parentObj = bannerElementWrapper.parentNode;
var elementClass = parentObj.className;
var searchForRow = true;
while ((elementClass.indexOf('row') == -1) && searchForRow) {
if (checkIfNodeInPage(parentObj.parentNode)) {
parentObj = parentObj.parentNode;
elementClass = parentObj.className;
} else {
searchForRow = false;
}
}
if (searchForRow) {
parentObj.className += ' jl-has-banner';
}
}
}
} else if (unitData['sizeType'] === 'outofpage') {
// fix za ispravan prikaz wallpapera na sportskim novostima
if ((dfpName === 'sn') && (unitData['unit'] === 'wp')) {
var bodyElement = document.querySelector('body');
bodyElement.className += ' hasWallpaperBanner';
}
}
}
});
googletag.enableServices();
});
// slušaj scroll event za potrebe učitvanja lazy loadom
window.addEventListener('scroll', dfpScrollHandler);
// inicijaliziraj sticky bannere
window.addEventListener('load', dfpInitStickyBanners);
window.addEventListener('resize', dfpInitStickyBanners);
var allowedPublications = ['jutarnjiList', 'sportskeNovosti', 'gloria'];

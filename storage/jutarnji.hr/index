function validateEmail(email) {
var re = /^(([^<>()\[\]\.,;:\s@\"]+(\.[^<>()\[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
return re.test(email);
}
function validateForm() {
var email = document.forms["mpNas"]["field1236"].value;
if (!validateEmail(email)) {
alert("Molimo upišite valjanu e-mail adresu");
return false;
}
}

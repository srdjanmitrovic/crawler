filing: function(elem) {
if ( elem.node.classList[1] === 'active' ) {
elem.select('#back').attr({
fill: '#000',
fillOpacity: 0
});
elem.select('#front').attr({
fill: '#ccc'
});
}
else {
elem.select('#back').attr({
fill: '#000',
fillOpacity: .1
});
elem.select('#front').attr({
fill: '#fff'
});
}
},
hoverIn: function() {
var self = this;
self.select('#back').animate({ fillOpacity: 0
}, 200, mina.easeout);
self.select('#front').animate({ fill: '#ccc'
}, 200, mina.easeout);
},
hoverOut: function() {
var self = this;
// if ( elem.node.classList[1] !== 'active' ) // {
self.select('#back').animate({ fillOpacity: .3
}, 200, mina.easeout);
self.select('#front').animate({ fill: '#fff'
}, 200, mina.easeout);
// }
}
};
var ovan = Snap("#ovan");
var bik = Snap("#bik");
var blizanci = Snap("#blizanci");
var rak = Snap("#rak");
var lav = Snap("#lav");
var djevica = Snap("#djevica");
var vaga = Snap("#vaga");
var skorpion = Snap("#skorpion");
var strijelac = Snap("#strijelac");
var jarac = Snap("#jarac");
var vodenjak = Snap("#vodenjak");
var riba = Snap("#riba");

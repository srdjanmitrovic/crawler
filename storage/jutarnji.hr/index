var dfpCountryCode = "HR";
var dfpPublicationName = "jutarnjiList";
var dfpCurrentPageType = "nas";
var dfpGalleryImgNum = 0;
var dfpGalleryUnitNum = 0;
var dfpCategory = "Front Page";
var dfpCategoryParent = "";
var dfpRos = "naslovnica";
var dfpArticle = "";
var dfpArticleKeyword = "";
// oglasne jedinice za trenutačnu stranicu
var dfpPlacementUnits = dfpUnits[dfpCurrentPageType];
// jedinice koje je potrebno osvježiti
var dfpRefreshUnits = [];
// jedinice koje je potrebno osvježiti a ulaze u prebid
var dfpRefreshPrebidUnits = [];
var dfpWindowWidth = dfpGetWindowWidth();
var dfpWindowDPR = dfpGetDevicePixelRatio;
var dfpIsMobile = false;
if (dfpWindowWidth <= dfpMobileMaxWidth) {
dfpIsMobile = true;
}
var dfpBreakpoint = dfpGetBreakpoint();
var dfpPrebid = true;
if (dfpCountryCode === 'HR') {
dfpPrebid = false;
}
var dfpPrebidTimeout = 3000;
var dfpPrebidUnits = [];
var dfpPrebidGranularity = {
"buckets" : [
{
"min" : 0,
"max" : 100,
"increment" : 0.1
}
]
};
var pbjs = pbjs || {};
pbjs.que = pbjs.que || [];
// inicijaliziraj sve oglasne jedinice ovisno o učitanoj stranici i postavljenim parametrima
googletag.cmd.push(function() {
dfpConfigUnits();
});
// dohvati omjer CSS i fizičkog piksela
function dfpGetDevicePixelRatio() {
var ratio = 1;
if ((window.screen.systemXDPI !== undefined) && (window.screen.logicalXDPI !== undefined) && (window.screen.systemXDPI > window.screen.logicalXDPI)) {
ratio = window.screen.systemXDPI / window.screen.logicalXDPI;
} else if (window.devicePixelRatio !== undefined) {
ratio = window.devicePixelRatio;
}
return ratio;
}
// dohvati širinu prozora
function dfpGetWindowWidth() {
var windowWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
return windowWidth;
}
// pronađi najmanji breakpoint za trenutačnu širinu prozora
function dfpGetBreakpoint() {
var breakpoints = [640, 768, 960, 1200, 1350, 1366, 1440, 1600, 1920, 2560];
var minBreakpoint = [640];
for (i = 0; i < breakpoints.length; i++) {
if (dfpWindowWidth >= breakpoints[i]) {
minBreakpoint = [breakpoints[i]];
}
}
return minBreakpoint;
}
// provjeri sadrži li DOM traženi node
function checkIfNodeInPage(node) {
var inDocument = false;
if (node === document.body) {
inDocument = false;
} else {
inDocument = document.body.contains(node);
}
return inDocument;
}
// čekaj određeno vrijeme na poziv callback funkcije prilikom scroll eventa
function dfpThrottleScrolling(callback, wait) {
var time;
var go = true;
return function() {
if (go) {
go = false;
time = setTimeout(function() {
time = null;
go = true;
callback.call();
}, wait);
}
};
};
// inicijaliziraj sticky bannere
function dfpInitStickyBanners() {
if (dfpWindowWidth >= dfpStickyMinWidth) {
// univerzalni preporučeni nazivi klasa
var stickyRowClass = '.dfp-scroll-row';
var stickyColClass = '.dfp-scroll-column';
var stickyElementClass = '.dfp-sticky-unit';
// stari nazivi klasa, trebaju se zamijeniti univerzalnima na svim portalima
if ((dfpName === 'jl') || (dfpName === 'sn')) {
stickyRowClass = '.jl-scroll-container';
stickyColClass = '.jl-banner-column';
stickyElementClass = '.jl-banner-wrapper';
}
var stickyElementContainers = document.querySelectorAll(stickyRowClass);
for (var i = stickyElementContainers.length - 1; i >= 0; i--) {
var contentHeight = stickyElementContainers[i].offsetHeight;
var stickyElementColumn = stickyElementContainers[i].querySelector(stickyColClass);
if (stickyElementColumn !== null) {
var stickyElement = stickyElementColumn.querySelector(stickyElementClass);
if (stickyElement !== null) {
//if (dfpWindowDPR === 1) {
var stickyElementHeight = stickyElement.offsetHeight;
if (stickyElementHeight >= dfpStickyUnitHeightLimit) {
if (contentHeight < dfpStickyColumnHeightLimit) {
contentHeight = dfpStickyColumnHeightLimit;
}
} else {
contentHeight = contentHeight - 20;
}
stickyElementColumn.style.height = contentHeight + 'px';
Stickyfill.add(stickyElement);
//} else {
//stickyElement.classList.remove("jl-sticky-banner");
//}
}
}
}
}
}
// handler lazy load učitavanja jedinica
var dfpScrollHandler = dfpThrottleScrolling(function() {
scrolled = window.scrollY;
// provjeri postoje li lazy load jedinice ili ukloni listener
if (dfpLazyLoad.length > 0) {
dfpLazyLoad.forEach(function(adUnit) {
var adUnitIndex = dfpLazyLoad.indexOf(adUnit);
var unitContainer = document.getElementById(dfpUnits[dfpCurrentPageType][adUnit]['container']);
if (checkIfNodeInPage(unitContainer)) {
var containerOffset = unitContainer.getBoundingClientRect().top;
if (containerOffset < dfpLazyUnitOffset) {
var slot = dfpUnits[dfpCurrentPageType][adUnit]['slot'];
googletag.cmd.push(function() {
googletag.pubads().refresh([ dfpAdSlots[slot] ]);
});
dfpLazyLoad.splice(adUnitIndex, 1);
}
}
});
} else {
window.removeEventListener('scroll', dfpScrollHandler);
}
}, 250);
function dfpConfigUnits() {
if (dfpPlacementUnits !== '') {
var dfpUnitCounter = 0;
for (var unitId in dfpPlacementUnits) {
if (dfpPlacementUnits.hasOwnProperty(unitId)) {
if ((dfpCurrentPageType === 'fot') && ((dfpUnitCounter >= dfpGalleryUnitNum) || (dfpUnitCounter >= 5))) {
break;
}
// podaci o trenutačnoj jedinici
var unitData = dfpPlacementUnits[unitId];
// prebid konfiguracija
var unitPrebidConfigParam = 'prebidConfig';
if (dfpIsMobile === true) {
unitPrebidConfigParam = unitPrebidConfigParam + 'Mobile';
}
// provjeri treba li biti inicijalizirana ovisno o širini ekrana
if (((dfpIsMobile === true) && (unitData['deviceType'] === 'mobile')) || ((dfpIsMobile === false) && (unitData['deviceType'] === 'desktop')) || (unitData['deviceType'] === 'all')) {
// drugačiji inicijalizacijski kod za out of page jedinice
if (unitData['unit'] == 'wp') {
dfpAdSlots[unitData['slot']] = googletag.defineOutOfPageSlot('/' + dfpId + '/' + dfpName + '_' + dfpCurrentPageType + '_' + unitData['unit'], unitId).addService(googletag.pubads());
} else {
dfpAdSlots[unitData['slot']] = googletag.defineSlot('/' + dfpId + '/' + dfpName + '_' + dfpCurrentPageType + '_' + unitData['unit'], unitData['size'], unitId).addService(googletag.pubads());
}
// targetiranje specifičnih veličina ekrana
var sizeKey = 'WindowSize';
dfpAdSlots[unitData['slot']].setTargeting(sizeKey, dfpBreakpoint);
// targetiranje po rubrici
if (dfpCategory !== '') {
dfpAdSlots[unitData['slot']].setTargeting('Category', dfpCategory);
}
if (dfpCategoryParent !== '') {
dfpAdSlots[unitData['slot']].setTargeting('CategoryParent', dfpCategoryParent);
}
// targetiranje za članke
if (dfpCurrentPageType === 'cla') {
if (dfpArticle !== '') {
dfpAdSlots[unitData['slot']].setTargeting('Article', dfpArticle);
}
if (dfpArticleKeyword !== '') {
dfpAdSlots[unitData['slot']].setTargeting('ArticleKeyword', dfpArticleKeyword);
}
}
// ros targetiranje
if (dfpRos !== '') {
dfpAdSlots[unitData['slot']].setTargeting('ROS', dfpRos);
}
// gledamo samo jedinice koje nisu u Prebidu
if ((dfpPrebid === true) && (unitData.hasOwnProperty(unitPrebidConfigParam) === true)) {
if (dfpDisableInitialLoad === true) {
dfpRefreshPrebidUnits.push(dfpAdSlots[unitData['slot']]);
}
} else {
if (unitData['lazyLoad']) {
dfpLazyLoad.push(unitId);
} else {
if (dfpDisableInitialLoad === true) {
dfpRefreshUnits.push(dfpAdSlots[unitData['slot']]);
}
}
}
}
}
dfpUnitCounter = dfpUnitCounter + 1;
}
}
}
function dfpConfigPrebidUnits() {
if (dfpPlacementUnits !== '') {
var dfpUnitCounter = 0;
for (var unitId in dfpPlacementUnits) {
if (dfpPlacementUnits.hasOwnProperty(unitId)) {
if ((dfpCurrentPageType === 'fot') && ((dfpUnitCounter >= dfpGalleryUnitNum) || (dfpUnitCounter >= 5))) {
break;
}
// podaci o trenutačnoj jedinici
var unitData = dfpPlacementUnits[unitId];
// provjeri treba li biti inicijalizirana ovisno o širini ekrana
if (((dfpIsMobile === true) && (unitData['deviceType'] === 'mobile')) || ((dfpIsMobile === false) && (unitData['deviceType'] === 'desktop')) || (unitData['deviceType'] === 'all')) {
// različita Prebid konfiguracija ovisno radi li se o mobilnim ili desktop uređajima
var unitPrebidConfigParam = 'prebidConfig';
if (dfpIsMobile === true) {
unitPrebidConfigParam = unitPrebidConfigParam + 'Mobile';
}
// ako je jedinica dostupna za bidding stavi ju u odgovarajući niz
if (unitData.hasOwnProperty(unitPrebidConfigParam)) {
var unitBids = [];
for (var bidderName in unitData[unitPrebidConfigParam]) {
var bidderParams = [];
bidderParams = unitData[unitPrebidConfigParam][bidderName];
// dodatni parametri za connectad
if (bidderName === 'connectad') {
bidderParams['networkId'] = '10047';
bidderParams['siteId'] = '977103';
} else if (bidderName === 'openx') {
bidderParams['delDomain'] = 'hanzamedia-d.openx.net';
}
var bidderLabels = ["desktop-" + unitData["sizeType"], "tablet-" + unitData["sizeType"]];
if ((dfpIsMobile === true) && ((unitData['deviceType'] === 'mobile') || (unitData['deviceType'] === 'all'))) {
bidderLabels = ["phone-" + unitData["sizeType"]];
}
var bidderData = {bidder: bidderName, params: bidderParams, labelAny: bidderLabels};
unitBids.push(bidderData);
}
var dfpPrebidUnit = {
code: unitId,
mediaTypes: {banner: {sizes: unitData['size']}},
bids: unitBids
}
if (bidderName !== 'sovrn') {
dfpPrebidUnits.push(dfpPrebidUnit);
}
}
}
}
dfpUnitCounter = dfpUnitCounter + 1;
}
}
}
// prebid
function dfpPrebidSendAdServerRequest() {
if (pbjs.initAdserverSet) return;
pbjs.initAdserverSet = true;
googletag.cmd.push(function() {
pbjs.que.push(function() {
pbjs.setTargetingForGPTAsync();
googletag.pubads().refresh(dfpRefreshPrebidUnits);
});
});
}
if (dfpPrebid === true) {
dfpConfigPrebidUnits();
if (dfpPrebidUnits.length > 0) {
pbjs.que.push(function() {
pbjs.setConfig({
currency: {
"adServerCurrency": "EUR"
//"conversionRateFile": "<url>"
},
debug: true,
priceGranularity: "medium",
enableSendAllBids: false,
bidderSequence: "random",
bidderTimeout: dfpPrebidTimeout,
userSync: {
//enabledBidders: ['openx'], // appnexus i connectad ne podržavaju
iframeEnabled: true
},
sizeConfig: [
{
'mediaQuery': '(min-width: 970px)',
'sizesSupported': [
[970, 250],
[970, 90],
[728, 90]
],
'labels': ['desktop-pagebreak', 'tablet-pagebreak']
},
{
'mediaQuery': '(min-width: 970px)',
'sizesSupported': [
[300, 600],
[300, 250],
],
'labels': ['desktop-aside', 'tablet-aside']
},
{
'mediaQuery': '(min-width: 0px) and (max-width: 969px)',
